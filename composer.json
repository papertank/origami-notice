{
    "name": "origami/notice",
    "description": "Flash notice package for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2",
        "illuminate/session": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Notice\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Notice\\NoticeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
